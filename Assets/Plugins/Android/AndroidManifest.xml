<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" package="com.ROBED.BuleVSColors">
  <application>
    <!-- Your launcher activity (exported because it has an intent-filter) -->
    <activity android:name="com.unity3d.player.UnityPlayerActivity" android:exported="true">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <!-- FIX for Vungle receiver -->
    <!-- Vungle’s manifest declares an intent-filter, so this must be exported=true -->
    <receiver android:name="com.vungle.warren.NetworkProviderReceiver" android:exported="true" tools:node="merge" tools:replace="android:exported" />
  </application>
</manifest>